model Student {
    studentId         Int               @id @default(autoincrement())
    studentUsername   String            @db.VarChar(45)
    studentStatusId   Int
    coursePlanId      Int
    fact_register     FactRegister[]
    fact_std_plan     FactStdPlan[]
    fact_term_summary FactTermSummary[]
    course_plan       CoursePlan        @relation(fields: [coursePlanId], references: [coursePlanId], onDelete: NoAction, onUpdate: NoAction, map: "fk_student_course_plan1")
    student_status    StudentStatus     @relation(fields: [studentStatusId], references: [studentStatusId], onDelete: NoAction, onUpdate: NoAction, map: "fk_student_student_status1")

    @@index([coursePlanId], map: "fk_student_course_plan1_idx")
    @@index([studentStatusId], map: "fk_student_student_status1_idx")
    @@map("student")
}

model StudentStatus {
    studentStatusId Int       @id @default(autoincrement())
    status          String    @db.VarChar(45)
    student         Student[]

    @@map("student_status")
}

model FactStdPlan {
    stdPlanId          Int           @id @default(autoincrement())
    studentId          Int
    subjectCourseId    Int
    gradeLabelId       Int?
    grade              String?       @db.VarChar(45)
    isPass             Int           @default(0) @db.TinyInt
    note               String?       @db.VarChar(45)
    semester           Int
    semesterPartInYear String        @db.VarChar(45)
    student            Student       @relation(fields: [studentId], references: [studentId], onDelete: NoAction, onUpdate: NoAction, map: "fk_fact-std-plan_student1")
    grade_label        GradeLabel?   @relation(fields: [gradeLabelId], references: [gradeLabelId], onDelete: NoAction, onUpdate: NoAction, map: "fk_fact_std_plan_grade_label1")
    subject_course     SubjectCourse @relation(fields: [subjectCourseId], references: [subjectCourseId], onDelete: NoAction, onUpdate: NoAction, map: "fk_fact_std_plan_subject_course1")

    @@index([studentId], map: "fk_fact-std-plan_student1_idx")
    @@index([gradeLabelId], map: "fk_fact_std_plan_grade_label1_idx")
    @@index([subjectCourseId], map: "fk_fact_std_plan_subject_course1_idx")
    @@map("fact_std_plan")
}
